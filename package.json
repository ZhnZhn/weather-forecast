{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "css:min": "node ut/css-min.js",
    "css:sass": "node ut/css-sass.js",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --color --progress --config webpack.dll.js json > stats-dll.json"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "dompurify": "^2.3.10",
    "interactjs": "^1.3.4",
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "recharts": "^2.1.13",
    "redux": "^4.1.2",
    "redux-saga": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "clean-css": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.55.0",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
