{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "css:min": "gulp",
    "css:sass": "gulp sass",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --colors --progress --config webpack.dll.js json > stats-dll.json"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "jspm": {
    "dependencies": {
      "babel-polyfill": "npm:babel-polyfill@^6.26.0",
      "interactjs": "npm:interactjs@^1.3.4",
      "leaflet": "npm:leaflet@1.3.4",
      "react": "npm:react@^16.12.0",
      "react-dom": "npm:react-dom@^16.12.0",
      "recharts": "npm:recharts@1.0.1",
      "redux": "npm:redux@^4.0.1",
      "redux-saga": "npm:redux-saga@^0.16.2",
      "regenerator-runtime": "npm:regenerator-runtime@^0.11.1"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "interactjs": "^1.3.4",
    "leaflet": "^1.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "recharts": "^1.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-recharts": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "del": "^4.0.0",
    "eslint": "^4.18.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-redux-saga": "^0.6.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jspm": "^0.16.55",
    "webpack": "^3.12.0"
  }
}
