{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "css:min": "gulp",
    "css:sass": "gulp sass",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --color --progress --config webpack.dll.js json > stats-dll.json"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "dompurify": "^2.3.1",
    "interactjs": "^1.3.4",
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "recharts": "^2.1.2",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "del": "^6.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "sass": "^1.38.2",
    "terser-webpack-plugin": "^5.2.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  }
}
