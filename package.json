{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "css:min": "gulp",
    "css:sass": "gulp sass",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --colors --progress --config webpack.dll.js json > stats-dll.json"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.7.7",
      "interactjs": "npm:interactjs@^1.3.4",
      "leaflet": "npm:leaflet@1.3.4",
      "react": "npm:react@^16.13.1",
      "react-dom": "npm:react-dom@^16.13.1",
      "recharts": "npm:recharts@1.0.1",
      "redux": "npm:redux@^4.0.1",
      "redux-saga": "npm:redux-saga@^0.16.2"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "interactjs": "^1.3.4",
    "leaflet": "^1.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-redux-saga": "^0.6.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
