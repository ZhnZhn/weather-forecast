{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Webapp Weather Forecast",
  "main": "index.js",
  "scripts": {
    "sass": "gulp sass",
    "gulp": "gulp",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.config-dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --colors --progress --config webpack.dll.js json > stats-dll.json"
  },
