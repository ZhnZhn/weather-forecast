{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "css:min": "node ut/css-min.js",
    "css:sass": "node ut/css-sass.js",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",    
    "stats:json": "webpack --config webpack.dll-jsx.js --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static",
    "stats:dll": "webpack --color --progress --config webpack.dll.js json > stats-dll.json"    
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "dompurify": "^2.4.5",
    "leaflet": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "recharts": "^2.5.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "html-webpack-plugin": "^5.5.1",
    "sass": "^1.62.1",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.82.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
