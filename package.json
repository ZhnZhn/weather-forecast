{
  "name": "weather-forecast",
  "version": "0.2.0",
  "description": "Web app Weather Forecast",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "css:min": "node ut/css-min.js",
    "css:sass": "node ut/css-sass.js",
    "css:all": "npm run css:sass && npm run css:min",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:dll": "webpack --color --progress --config webpack.dll.js json > stats-dll.json"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "dompurify": "^2.4.4",
    "leaflet": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "recharts": "^2.1.14",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.58.3",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
